{
  "$schema": "https://turborepo.dev/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "lint": {},
    "format": {
      "cache": false
    },
    "format:check": {},
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "homey:run": {
      "cache": false,
      "persistent": true
    },
    "homey:install": {
      "cache": false
    },
    "homey:build": {},
    "homey:publish": {
      "cache": false,
      "interactive": true
    },
    "test": {
      "cache": false
    }
  }
}